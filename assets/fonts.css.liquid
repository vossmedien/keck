@font-face {
  font-display: swap;
  font-family: 'Barlow';
  font-style: normal;
  font-weight: 400;
  src: url('{{ "barlow-v12-latin-regular.woff2" | asset_url }}') format('woff2'),
       url('{{ "barlow-v12-latin-regular.ttf" | asset_url }}') format('truetype');
}

